{
  "name": "gumga-many-to-many-ng",
  "version": "1.4.0",
  "description": "Gumga Many To Many",
  "main": "./src/many-to-many.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack.dev.config.js",
    "prod": "NODE_ENV=production webpack --config ./webpack.production.config.js && webpack --config ./webpack.production.bundle.config",
    "doc": "node ./docs/create-doc.js"
  },
  "keywords": [
    "gumga",
    "many to many",
    "angular 1.x"
  ],
  "dependencies": {
    "angular": "1.5.8"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      [
        "uglifyify",
        {
          "mangle": false,
          "compress": {
            "sequences": true,
            "dead_code": true,
            "booleans": true,
            "conditionals": true,
            "if_return": false,
            "drop_console": false,
            "keep_fnames": true
          },
          "output": {
            "comments": false
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.28.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gumga-controller-ng": "^1.1.5",
    "gumga-rest-ng": "^1.1.5",
    "script-loader": "^0.7.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "unminified-webpack-plugin": "^1.1.1",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-stream": "^3.2.0"
  },
  "author": "Gumga Tecnologia da Informação S/A",
  "license": "LGPL-3.0"
}
